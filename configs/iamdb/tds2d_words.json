{
  "seed" : 0,
  "data" : {
    "dataset" : "iamdb",
    "data_path" : "<data_path>",
    "num_features" : 64,
    "use_words" : true
  },
  "model_type" : "tds2d",
  "model" : {
    "depth" : 4,
    "tds_groups" : [
      { "channels" : 4, "num_blocks" : 3, "stride" : [2, 2] },
      { "channels" : 16, "num_blocks" : 3, "stride" : [2, 2] },
      { "channels" : 32, "num_blocks" : 3, "stride" : [2, 1] },
      { "channels" : 64, "num_blocks" : 3, "stride" : [2, 1] }
    ],
    "kernel_size" : [5, 7],
    "dropout" : 0.1
  },
  "optim" : {
    "batch_size" : 128,
    "epochs" : 400,
    "learning_rate" : 5e-1,
    "step_size" : 100,
    "max_grad_norm" : 10
  }
}
